  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.6",
    "@types/node": "^24.9.1",
    "cypress": "^15.5.0",
    "esbuild": "^0.25.11",
    "multiple-cucumber-html-reporter": "^3.9.3",
    "typescript": "^5.9.3"
  }
}